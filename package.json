{
  "name": "dekamara.client",
  "version": "1.0.3",
  "description": "A feature-rich client for kirka.io",
  "keywords": [
    "kirka",
    "client",
    "io-games",
    "io",
    "electron"
  ],
  "homepage": "https://github.com/NamekujiLSDs/DekamaraClient#readme",
  "bugs": {
    "url": "https://github.com/NamekujiLSDs/DekamaraClient/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NamekujiLSDs/DekamaraClient.git"
  },
  "license": "GPL-3.0-or-later",
  "author": "NamekujiLSDs",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "test": "npx electron .",
    "build": "electron-builder --win --x64 --publish never",
    "publish": "electron-builder --win --x64 --publish always"
  },
  "devDependencies": {
    "electron": "^10.4.7",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
    "discord-rpc": "^3.2.0",
    "electron-localshortcut": "^3.2.1",
    "electron-log": "^5.4.3",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.1.7",
    "node-fetch": "^3.3.2",
    "path": "^0.12.7",
    "v8-compile-cache": "^2.4.0"
  },
  "build": {
    "appId": "deka.mara.client",
    "directories": {
      "output": "dist"
    },
    "win": {
      "publish": {
        "provider": "github",
        "owner": "NamekujiLSDs"
      },
      "target": "nsis",
      "icon": "/src/img/icon.ico"
    },
    "nsis": {
      "oneClick": true
    }
  }
}